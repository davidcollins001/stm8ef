: SHL ( n - n ) \ logical left shift
  [
    $9093 ,  \     LDW     Y,X
    $FE  C,  \     LDW     X,(X)
    $59  C,  \     RLCW    X
    $51  C,  \     EXGW X,Y
    $FF  C,  \     LDW (X),Y
  ]
  ;
